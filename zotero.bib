@article{Cho2001,
  title = {Impedance Imaging of Two-Phase Flow Field with Mesh Grouping Method},
  author = {Cho, Kyung Ho and Kim, Sin and Lee, Yoon Joon},
  year = {2001},
  month = feb,
  volume = {204},
