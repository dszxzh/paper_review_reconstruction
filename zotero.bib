@article{Greenwood2015,
  title = {Design of Ultrasonic Attenuation Sensor with Focused Transmitter for Density Measurements of a Slurry in a Large Steel Pipeline},
  author = {Greenwood, Margaret Stautberg},
  year = {2015},
  month = dec,
  volume = {138},
  pages = {3846--3854},
  publisher = {{Acoustical Society of America}},
  issn = {0001-4966},
  doi = {10.1121/1.4937767},
  abstract = {To design an ultrasonic sensor to measure the attenuation and density of slurry carried by a large steel pipeline (diameter up to 70\,cm) is the goal of this research. The pitch-catch attenuation sensor, placed in a small section of the pipeline, contains a send unit with a focused transducer that focuses the ultrasound to a small region of the receive unit on the opposite wall. The focused transducer consists of a section of a sphere (base {$\sim$}12\,cm) on the outer side of the send unit and a 500\,kHz piezoelectric shell of lead zirconate titanate epoxied to it. The Rayleigh surface integral is used to calculate the pressure in steel and in water (slurry). An incremental method to plot the paths of ultrasonic rays shows that the rays focus where expected. Further, there is a region where the parallel rays are perpendicular to the wall of the receive unit. Designs for pipeline diameters of 25 and 71\,cm show that the pressure in water at the receive transducer is about 17 times that for a pitch-catch system using 5\,cm diameter disk transducers. The enhanced signal increases the sensitivity of the measurements and improves the signal-to-noise ratio.},
  journal = {The Journal of the Acoustical Society of America},
  number = {6}
}

@article{Greenwood2018,
  title = {Attenuation Measurements with Ultrasonic Diffraction Grating Show Dependence upon Particle Size of Slurry and Viscosity of Base Liquid},
  author = {Greenwood, Margaret Stautberg},
  year = {2018},
  month = mar,
  volume = {84},
  pages = {134--149},
  issn = {0041-624X},
  doi = {10.1016/j.ultras.2017.10.003},
  abstract = {How can using an ultrasonic diffraction grating lead to slurry characterization? The diffraction grating, which is formed by machining triangular grooves on the flat surface of an aluminum unit, has send and receive transducers fastened to the unit at an angle of 30\textdegree. The ultrasonic beam strikes the back of the grating, in contact with the slurry, and reflects a beam to the receive transducer; m = 0 and m = 1 beams are transmitted into the slurry. The angle of the m = 1 beam changes with frequency and, at the critical frequency fCR, it reaches 90\textdegree. When f {$<$} fCR, the m = 1 beam disappears, its energy is shared with all other beams, producing a peak in the receive transducer. The change in peak height with slurry concentration determines the attenuation; the frequency at the peak yields the velocity of sound. The attenuation has been measured for polystyrene spheres, ranging in size from 98 \textmu m to 463 \textmu m, and slurry concentrations up to 20 wt\%. When the spheres are immersed in water, sugar water, or mineral oil, the attenuation measurements show the effect of particle diameter and the viscosity of the base fluid.},
  journal = {Ultrasonics},
  keywords = {Attenuation measurements,Density,Particle size,Ultrasonic diffraction grating,Velocity of sound,Viscosity},
  language = {en}
}

