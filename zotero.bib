@article{ZhiyaoHuang2003,
  title = {Application of Electrical Capacitance Tomography to the Void Fraction Measurement of Two-Phase Flow},
  author = {Zhiyao Huang and Baoliang Wang and Haiqing Li},
  year = {2003},
  month = feb,
  volume = {52},
  pages = {7--12},
  issn = {1557-9662},
  doi = {10.1109/TIM.2003.809087},
  abstract = {Based on the electrical capacitance tomography technique, a new method for the void fraction measurement of two-phase flow is proposed. A 12-electrode void fraction measurement system is established. A mathematical model of image reconstruction of electrical capacitance tomography is developed. To obtain the quantitative information of two-phase flow, combining the Tikhonov regularization principle and the algebraic reconstruction technique algorithm, a new image reconstruction algorithm is presented. The experimental results show that the accuracy of void fraction measurement is satisfactory. The proposed method is suitable for the void fraction measurement of many kinds of two-phase flow.},
  annotation = {ZSCC: 0000189},
  journal = {IEEE Transactions on Instrumentation and Measurement},
  keywords = {algebraic reconstruction technique algorithm,capacitance measurement,Capacitance measurement,Capacitive sensors,Data acquisition,Dielectric measurements,electric impedance imaging,Electric variables measurement,electrical capacitance tomography,Electrical capacitance tomography,Electrodes,flow measurement,Fluid flow measurement,image reconstruction,Image reconstruction,mathematical model,Permittivity measurement,Tikhonov regularization,two-phase flow,void fraction measurement},
