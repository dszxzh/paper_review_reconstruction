@article{Mwambela2001,
  title = {Multiphase Flow Component Volume Fraction Measurement: Experimental Evaluation of Entropic Thresholding Methods Using an Electrical Capacitance Tomography System},
  shorttitle = {Multiphase Flow Component Volume Fraction Measurement},
  author = {Mwambela, A. J. and Johansen, G. A.},
