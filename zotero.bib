@article{Fossa1998,
  ids = {Fossa1998a},
  title = {Design and Performance of a Conductance Probe for Measuring the Liquid Fraction in Two-Phase Gas-Liquid Flows},
  author = {Fossa, M.},
  year = {1998},
  month = jun,
  volume = {9},
  pages = {103--109},
  publisher = {{Elsevier}},
  issn = {09555986},
  doi = {10.1016/S0955-5986(98)00011-9},
  abstract = {The performance of ring-shaped and plate electrodes for measuring the conductance of gas-liquid mixtures in pipes is studied experimentally and the results are compared with the theory available in the literature. The device set up consists of electrode pairs placed on the internal wall of a cylindrical test duct, flush to the pipe surface. An a.c. carrier voltage of 20 kHz frequency is applied across each pair of electrodes. An electronic device converts the a.c. signal into a d.c. signal proportional to the impedance of the two-phase test region in order to obtain the local liquid fraction values. Moreover the effects of the probe geometry upon the response of the measuring device have also been investigated with reference to annular, stratified and bubble phase distributions so as to broaden the scope of the available data. \textcopyright{} 1998 Elsevier Science Ltd. All rights reserved.},
  journal = {Flow Measurement and Instrumentation},
