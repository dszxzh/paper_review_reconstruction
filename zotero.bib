@article{Hu2017,
  title = {A {{Smart Electrostatic Sensor}} for {{Online Condition Monitoring}} of {{Power Transmission Belts}}},
  author = {Hu, Yonghui and Zhang, Shuai and Yan, Yong and Wang, Lijuan and Qian, Xiangchen and Yang, Lu},
  year = {2017},
  month = sep,
  volume = {64},
  pages = {7313--7322},
  issn = {0278-0046, 1557-9948},
  doi = {10.1109/TIE.2017.2696507},
  abstract = {Online condition monitoring of power transmission belts is essential to keep industrial belt-driven equipment functioning smoothly and reliably. This paper presents a smart electrostatic sensor that monitors belt motion through detection of static charge on the belt. A theoretical model is established using the method of moments for calculation of induced charge on strip-shaped electrodes placed in the vicinity of a belt moving both axially and transversely. The sensor unit converts the induced charge into proportional voltage signals using charge amplifiers and measures belt speed and vibration through cross correlation and spectral analysis, respectively. The performance of the smart electrostatic sensor is validated against a photoelectric rotary encoder and a laser displacement sensor. Comparative experimental results show that the belt speed can be measured with a relative error within {$\pm$}2\% over the range 1.7\textendash 15.5 m/s. The electrostatic sensor is capable of measuring the frequencies of transverse vibration accurately. Although absolute displacement cannot be measured due to the uncertain level of charge on the belt, the measurement results of relative vibration magnitudes for different modes and at different belt speeds are reasonably accurate.},
  journal = {IEEE Transactions on Industrial Electronics},
  language = {en},
  number = {9}
}

@incollection{Hu2018,
  title = {Evaluation of the {{Cross Talking Effect}} in {{Piezoresistive Tactile Sensor Matrices}}},
  booktitle = {2018 15th {{International Multi}}-{{Conference}} on {{Systems}}, {{Signals}} and {{Devices}} (Ssd)},
  author = {Hu, Zheng and Rajendran, Dhivakar and Wendler, Frank and Ramalingame, Rajarajan and Kanoun, Olfa},
  year = {2018},
  pages = {632--635},
  publisher = {{Ieee}},
  address = {{New York}},
  abstract = {Cross talking effects occur during measurements of sensor resistances in a two dimensional matrix. The cross talking current from neighborhood units influences measured values on the target sensor. This causes big systematic measurement deviations, especially for the piezoresistive sensors based on multi-walled carbon nanotube within Polydimethylsiloxane (PDMS). For this material, resistance value decreases significantly corresponding to the pressure value, which leads to large current increasing under the same excitation signal. In this paper, the cross talking effect is evaluated through the comparison between one usual data acquisition system and the data acquisition system equipped with voltage feedback circuit (VFC), which is suitable for the used nano-material. As result, it is demonstrated that voltage feedback method can eliminate the cross talking effect in the two-dimensional nano-composite sensor matrix. Meanwhile, the voltage feedback circuit cannot always remain high accuracy in extreme high resistance load range because of the non-ideal behavior of the hardware elements.},
  annotation = {WOS:000459847300111},
  isbn = {978-1-5386-5305-0},
  language = {English}
}

