@article{Holden1998,
  title = {Imaging Stirred-Vessel Macromixing Using Electrical Resistance Tomography},
  author = {Holden, P. J. and Wang, M. and Mann, R. and Dickin, F. J. and Edwards, R. B.},
  year = {1998},
