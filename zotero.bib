  issn = {0018-9456, 1557-9662},
  doi = {10.1109/TIM.2019.2923864},
  abstract = {This paper proposes a novel approach whereby fuzzy logic and decision tree are utilised to overcome the challenges in analysing images of gas-oil-water pipeline flow obtained using electrical resistance and capacitance dual-modality tomography. The approach firstly generates two axially-stacked concentration images from two stacks of the cross-sectional concentration tomograms reconstructed from different modalities respectively, and then registers two generated images in temporal and spatial terms. Afterwards, a fuzzy logic method is applied to perform a pixel-level fusion to integrate the registered images based on the characteristics of electrical tomograms for multiphase pipeline flow. Later, a decision tree is utilised to derive the local concentration of each individual phase according to the fusion results. Using the data from real industrial cases, both feasibility and robustness of the proposed approach are demonstrated. In addition, the proposed approach also overcomes the limitations of conventional threshold-based methods on the request of priori knowledge for the qualitative and quantitative analyses of gas-oil-water pipeline flow.},
  journal = {IEEE Transactions on Instrumentation and Measurement},
  language = {en},
  number = {5}
}

