  author = {Pulavskyi, Anatolii A. and Krivenko, Sergey S. and Kryvenko, Liudmyla S.},
  year = {2017},
  month = jun,
  pages = {1--4},
  issn = {null},
  doi = {10.1109/MECO.2017.7977241},
  abstract = {Nowadays, development of Internet of things (IoT), sensor networks, cyberspace has expanded to a smart learning environment. IoT provides new opportunities for Health Care. In the paper, we focus on diagnostics of diseases based on database with support of the IoT tools. Firstly, a mathematical database analysis technique that transforms a client request into an equivalent response of network with one more node based on client-server architecture written in Python and MATLAB is proposed. Then, a database analysis technique that employs machine learning with certain additional constraints is implemented. Experimental results show the efficiency and stability of the system.}
}

@article{Pusppanathan2017,
  ids = {Pusppanathan2017a},
  title = {Single-{{Plane Dual Modality Tomography}} for {{Multiphase Flow Imaging}} by {{Integrating Electrical Capacitance}} and {{Ultrasonic Sensors}}},
  author = {Pusppanathan, J. and Rahim, R. A. and Phang, F. A. and Mohamad, E. J. and Ayob, N. M. Nor and Rahiman, M. H. Fazalul and Seong, C. K.},
  year = {2017},
  volume = {PP},
  pages = {1--1},
  abstract = {This paper presents an experimental study of a single-plane dual-modality tomography system (DMT) integrating electrical capacitance tomography (ECT) and ultrasonic tomography (UT) for water-oil-gas three-phase flow imaging. The soft-field (ECT) and hard-field (UT) systems measure different physical parameters making them both a fine combination in complementing each system鈥檚 limitations. The DMT system consists of 16 units of customized ECT sensors and 16 units of UT transducers which are composited on a single-plane arrangement to perform concurrent measurement. A finite element method is used to model and study its feasibility and performance before engaging with the actual hardware. The DMT system is carried out on a vertical test column and horizontal stratified flow to obtain sensor data from each modality by inspection of separate tomographic image using linear back projection (LBP) image reconstruction algorithm and convolution back-projection with Gaussian filtering (CBPF). A fuzzy logic pixel fusion method is used for image fusion process. To evaluate the reconstructed images, the mean structural similarity index (MSSIM) method and mutual information method (MF) are employed on both the LBP and CBPF image results. The analysis and assessments proven that CBPF method has successfully improvised the image results thus has better MSSIM rating and MF over LBP method with enhancement up to 12.55\% and 8.92\% respectively.},
  journal = {IEEE Sensors Journal},
  keywords = {acoustic tomography,Acoustics,Capacitance,capacitance measurement,Capacitive sensors,CBPF,concurrent measurement,convolution back projection with Gaussian filtering,customized ECT sensors,DMT system,electrical capacitance tomography,Electrodes,finite element analysis,Finite element analysis,finite-element method,flow visualisation,fuzzy logic,fuzzy logic pixel fusion method,hard-field system,horizontal stratified flow,image filtering,image fusion,image fusion process,image reconstruction,LBP,linear back-projection image reconstruction algorithm,mean structural similarity index method,mechanical engineering computing,MSSIM,Multi-modality,multiphase flow imaging,mutual information method,sensor fusion,sensors,separate tomographic image inspection,single-plane arrangement,single-plane dual-modality tomography,soft-field system,stratified flow,three-phase flow imaging,tomography,Tomography,two-phase flow,ultrasonic imaging,ultrasonic sensors,ultrasonic tomography,ultrasonic transducers,UT transducers,vertical test column},
  number = {99}
}

