@article{Cao2011,
  title = {Direct Image Reconstruction for Electrical Capacitance Tomography by Using the Enclosure Method},
  author = {Cao, Zhang and Xu, Lijun},
  year = {2011},
  month = oct,
  volume = {22},
  pages = {104001},
  issn = {0957-0233, 1361-6501},
  doi = {10.1088/0957-0233/22/10/104001},
  abstract = {The enclosure method was introduced to electrical capacitance tomography for sensors of non-circular cross sections in the paper. Unlike the traditional methods based on the sensitivity theorem, it is a non-iterative algorithm for image reconstruction, as no iterations were required to obtain a rough estimation of the inclusions, i.e. the convex hull of the inclusions. Since the indicator function was calculated according to sensors of circular cross sections, the reconstruction process was simple and time efficient. Image reconstructions were implemented for sensors of non-circular cross sections by using the conformal transformation. The proposed method is suitable for the shape reconstruction of the convex hull, especially if the inclusion is in the center of the region. Both simulated and experiment results validated the feasibility and effectiveness of the enclosure method for electrical capacitance tomography.},
  journal = {Measurement Science and Technology},
  language = {en},
  number = {10}
}

