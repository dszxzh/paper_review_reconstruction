  year = {2020},
  month = jul,
  volume = {159},
  pages = {107773},
  issn = {0263-2241},
  doi = {10.1016/j.measurement.2020.107773},
  abstract = {Bearing steel ball is the basic part of rolling bearing, and its surface quality has a vital impact on the service performance, life and safety of rolling bearing. Therefore, the surface quality of bearing steel balls must be fully tested before leaving the factory. The unfolding mechanism and inspection sensor of steel ball surface are mainly described in this paper. First of all, the inspection methods of steel ball surface quality are analyzed in detail, including visual inspection, eddy current inspection, optical fiber sensor inspection, capacitance sensor inspection, ultrasonic sensor inspection, etc. Then, the methods of steel ball surface unfolding methods are introduced, including meridian unfolding method, one-dimensional orbit unfolding method, disk unfolding method, one-dimensional wheel unfolding method and rotating magnetic field unfolding method. Finally, the future development direction of steel ball detection technology is pointed out, which provides theoretical basis and technical reference for the realization of bearing steel ball surface micro-defect inspection system.},
  journal = {Measurement},
  keywords = {Capacitance sensor,Eddy current,Nondestructive testing,Optical fiber sensor,Steel ball,Ultrasonic,Visual inspection},
  language = {en}
}

@inproceedings{Zhang2020a,
  title = {Numerical {{Simulation}} of {{Effective Medium Approximation Using Monte Carlo Method}} and {{Its Experimental Evaluation}}},
  booktitle = {2020 {{IEEE International Instrumentation}} and {{Measurement Technology Conference}} ({{I2MTC}})},
  author = {Zhang, Qian and Cui, Ziqiang and Xia, Zihan and Yan, Long and Wang, Huaxiang},
  year = {2020},
  month = may,
  pages = {1--6},
  issn = {2642-2077},
  doi = {10.1109/I2MTC43012.2020.9128767},
  abstract = {The phase fraction in the industrial multi-phase flows is a key factor affecting the process efficiency and safety. The capacitance and other impedance based sensors can be employed to estimate this parameter by resorting to the effective medium approximation (EMA) method, which describes the macroscopic properties of composite materials with analytical or theoretical modeling. The EMAs have been paid increasingly attentions in determining the phase fraction in the multi-phase flow. There exist several different EMA models that can be utilized for phase fraction measurement, i.e., Maxwell-Garnett, Bruggeman and B\"ottcher models. It is essential to evaluate feasibility of these EMA models in gas-solid two-phase flows with different flow regimes, i.e., the homogenous and laminar flows. A four-electrode capacitance sensor is evaluated to validate multiple EMA models for estimating the solid fraction by numerical simulations and static experiments. The numerical simulation work focuses on comparing the solid fraction results obtained from different EMA models by Monte Carlo method.},
  keywords = {capacitance sensor,Effective medium approximation,Monte Carlo method,solid fraction.}
}

