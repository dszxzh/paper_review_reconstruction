@article{Tyagi2017,
  title = {Dense Gas\textendash Liquid\textendash Solid Flow in a Slurry Bubble Column: {{Measurements}} of Dynamic Characteristics, Gas Volume Fraction and Bubble Size Distribution},
  shorttitle = {Dense Gas\textendash Liquid\textendash Solid Flow in a Slurry Bubble Column},
  author = {Tyagi, Parul and Buwa, Vivek V.},
  year = {2017},
  month = dec,
  volume = {173},
  pages = {346--362},
  issn = {00092509},
  doi = {10.1016/j.ces.2017.07.042},
  abstract = {Dense three-phase slurry flows are important in several industrial processes (e.g., Methanol/ DME synthesis, Fischer Tropsch synthesis). In the present work, gas\textendash liquid\textendash solid flow in a pseudo 3D slurry bubble column is characterized under dense flow conditions for superficial gas velocity (UG) of 5\textendash 30 cm s\`A1 and overall solid loading (haSi) of 0\textendash 40 vol.\%. In-house developed voidage probes were used to measure local gas volume fraction fluctuations. The high frequency fluctuations (\$1\textendash 10 Hz) caused by individual bubbles or swarms of bubbles were found to increase with increase in haSi and UG. The low frequency oscillations (\${$<$} 1 Hz) corresponding to column-scale recirculating flow were found to decrease with increase in haSi. Importantly, time-averaged gas volume fraction and its internal composition (i.e. gas volume fraction contained in different bubble size groups) and bubble size distribution were measured at different locations in the slurry bubble column. With an increase in haSi, local gas volume fraction was found to decrease, accompanied by an increase in bubble size. Based on the distribution of bubble chord lengths, the gas volume fraction contained in different bubble size groups was measured for different haSi and UG. With increase in haSi, the gas volume fraction for small bubbles was found to decrease while that for large bubbles was found to increase. Under the dense flow conditions (at high UG and haSi), while small bubbles were found to accumulate near the column walls, as indicated by the wall peaking in aG profiles, large bubbles/gas slugs were found to flow predominantly through the column center. In addition to the new experimental data, the present work helps to improve the understanding of dense slurry flows that will aid the development and verification of Eulerian multiphase models.},
  journal = {Chemical Engineering Science},
  language = {en}
}

