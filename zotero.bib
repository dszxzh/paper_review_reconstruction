@article{Ahmed2006a,
  title = {Capacitance {{Sensors}} for {{Void}}-{{Fraction Measurements}} and {{Flow}}-{{Pattern Identification}} in {{Air}}\textendash{{Oil Two}}-{{Phase Flow}}},
  author = {Ahmed, H.},
