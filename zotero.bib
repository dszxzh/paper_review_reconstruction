@article{Ma2020,
  ids = {Ma2020a},
  title = {Spectral {{Capacitively Coupled Electrical Resistivity Tomography}} for {{Breast Cancer Detection}}},
  author = {Ma, Gege and Soleimani, Manuchehr},
  year = {2020},
