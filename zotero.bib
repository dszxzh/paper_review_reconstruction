@article{Shao2018,
  title = {{{RF}} Signal Detection by a Tunable Optoelectronic Oscillator Based on a {{PS}}-{{FBG}}},
  author = {Shao, Yuchen and Han, Xiuyou and Li, Ming and Zhao, Mingshan},
  year = {2018},
