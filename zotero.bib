@inproceedings{Chen2010a,
  ids = {Chen2010b},
  title = {Comparison of Three Electrical Capacitance Tomography Systems},
  booktitle = {2010 {{IEEE International Conference}} on {{Imaging Systems}} and {{Techniques}}},
  author = {Chen, D. and Yang, W. and Deng, X.},
  year = {2010},
  month = jul,
  pages = {57--62},
  issn = {1558-2809},
  doi = {10.1109/IST.2010.5548441},
  abstract = {Electrical capacitance tomography (ECT) has been developed for two decades to visualize the internal behavior of industrial processes containing dielectric components. Three different ECT systems have been developed: (1) based on a charge/discharge capacitance measuring circuit, (2) based on an AC circuit and (3) based on an impedance analyzer. In this paper the operation principles, calibration methods and some performance of the three systems are compared. The experimental results prove that the AC-based ECT system is superior to the charge/discharge ECT system. Although the impedance analyzer based ECT system can provide the highest accuracy and the best image quality, it is bulky and expensive.},
  keywords = {AC circuit,calibration,Calibration,capacitance measurement,Capacitance measurement,Charge measurement,charge-discharge capacitance measuring circuit,charge/discharge,Circuits,Current measurement,dielectric components,dielectric devices,Dielectric measurements,ECT,ECT system,electric impedance imaging,Electrical capacitance tomography,Image analysis,image quality,impedance analyzer,Impedance measurement,industrial processes,three electrical capacitance tomography systems,tomography,Visualization}
}

