  journal = {Ieee Transactions on Instrumentation and Measurement},
  keywords = {2-D median filter (MF),2D median filter,additive noise,demodulation,Demodulation,electric impedance imaging,electrical impedance tomography,electrical impedance tomography (EIT),Gaussian noise,image denoising,image filtering,image reconstruction,Image reconstruction,impulse noise,impulsive noise reduction,matched filters,median filters,medical image processing,multifrequency phase-sensitive demodulation,multifrequency phase-sensitive demodulation (PSD),multifrequency PSD,multifrequency signal,Noise reduction,numerical simulations,optimal linear filter,Robustness,Signal processing algorithms,Signal to noise ratio,Tomography},
  number = {1}
