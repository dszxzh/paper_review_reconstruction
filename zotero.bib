@article{Wang2016a,
  title = {Imaging Wet Granules with Different Flow Patterns by Electrical Capacitance Tomography and Microwave Tomography},
  author = {Wang, H. G. and Zhang, J. L. and Ramli, M. F. and Mao, M. X. and Ye, J. M. and Yang, W. Q. and Wu, Z. P.},
  year = {2016},
