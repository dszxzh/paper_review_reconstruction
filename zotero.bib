}

@article{Ghosh2012,
  title = {Identification of Flow Regimes Using Conductivity Probe Signals and Neural Networks for Counter-Current Gas\textendash Liquid Two-Phase Flow},
  author = {Ghosh, S. and Pratihar, D.K. and Maiti, B. and Das, P.K.},
  year = {2012},
  month = dec,
  volume = {84},
  pages = {417--436},
  issn = {00092509},
  doi = {10.1016/j.ces.2012.08.042},
  abstract = {A rigorous experimental investigation has been carried out on counter-current air\textendash water two-phase flow in vertical tube with circular cross-section over a wide range of phase flow rates of air and water. It is a novel attempt to capture, identify and predict the individual flow regimes for different combinations of air and water superficial velocities based on some of its objective descriptions. Two different conductivity probes (parallel wire and ring probes) have been designed to collect objective descriptions of the flow regime in terms of the time series voltage signals. Statistical analyses have been carried out to characterize those time series voltage signals in terms of some statistical parameters. Three methodologies have been developed using a genetic algorithm (GA)-tuned multi-layer feedforward neural network with back-propagation algorithm (GA-MLFFNN), a GA-tuned multi-layer radial basis function neural network with entropy-based clustering and back-propagation algorithm (GA-MLRBFNN\textendash EC) and lastly, a GA-tuned multi-layer radial basis function neural network with fuzzy C-means clustering and back-propagation algorithm (GA-MLRBFNN\textendash FCMC) to predict the different flow regimes after knowing the objective descriptions of the flow pattern in terms of the statistical parameters. Parallel wire conductivity probe is found to be better to capture the objective signature of the individual flow regime. GA-MLRBFNN\textendash EC is found to be the best to predict flow regimes successfully.},
  journal = {Chemical Engineering Science},
  language = {en}
}

@article{Giguere2008,
