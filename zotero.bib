@article{Rao2020,
  title = {A 1 {{MHz Miniaturized Electrical Impedance Tomography System}} for {{Prostate Imaging}}},
  author = {Rao, A. and Murphy, E. K. and Halter, R. J. and Odame, K. M.},
  year = {2020},
  month = aug,
  volume = {14},
  pages = {787--799},
  issn = {1940-9990},
  doi = {10.1109/TBCAS.2020.2994297},
  abstract = {An ASIC for a high frequency electrical impedance tomography (EIT) imaging system for prostate cancer screening is presented. The ASIC enables a small form-factor architecture, which ensures high signal-to-noise ratio (SNR) at MHz frequencies. The 4-channel ASIC was designed and fabricated in a standard CMOS 0.18-{$\mu$}m technology and integrates a novel current driver for current stimulus, instrumentation amplifier to interface with the tissue, VGA to provide variable gain and ADC with SPI interface for digitization. A prototype miniaturized EIT system was built and it was evaluated using a model transrectal imaging probe immersed into a tank filled with saline and a metal inclusion that demonstrated the open-domain problem of imaging prostate cancer lesion. The system maintained an SNR between 66 and 76 dB over the frequency range of 500 Hz to 1 MHz. Also, it produced reconstructed EIT images that depicted the presence of the small metal inclusion that modeled a prostate cancer imaging application.},
  journal = {IEEE Transactions on Biomedical Circuits and Systems},
  keywords = {application specific integrated circuit (ASIC),biomedical instrumentation,Biomedical measurement,Electrical impedance tomography (EIT),Electrodes,hardware system design,imaging,Probes,prostate imaging,Signal to noise ratio,Tomography,Voltage measurement},
  number = {4}
}

@article{Raouf2015,
  ids = {Raouf2015a},
  title = {Measurement of Capacitance and Resistance Using Two Perfectly Synchronized Voltage Sources},
  author = {Raouf, M. Helmy A. and Kim, Kyu Tae and Kim, Mun Seog},
  year = {2015},
  volume = {60},
  pages = {174--177},
  abstract = {A programmable bridge has been constructed by using two arbitrary waveform generators and DVM as a null-detector for capacitance and resistance measurements. The generators are controlled by a software program which has been specially designed to achieve their optimum performance. Methodology of this bridge has been introduced demonstrating the best way to get accurate ratio measurements up to the accuracy level of 10鈭5. Our bridge has been used, through this research, to get resistance to resistance and capacitance to resistance ratios at different frequencies. Validation of this bridge has been carried out through comparisons with another conventional bridge, which show satisfying agreement. In addition, the bridge has been verified by calculating its asymmetry which is about 0.65ppm in some cases. Finally, the uncertainty budget for the bridge measurements has been introduced in details.},
  journal = {Measurement},
  keywords = {Accuracy level of 10−5,Programmable bridges,Resistance and capacitance measurements,Synchronized voltage sources}
}

@article{Rasel2017,
  ids = {Rasel2017c},
  title = {Toward {{Multiphase Flow Decomposition Based}} on {{Electrical Capacitance Tomography Sensors}}},
  author = {Rasel, Rafiul K. and Zuccarelli, Christopher E. and Marashdeh, Qussai M. and Fan, Liang-Shih and TeixeiraIEEE, Fernando L.},
  year = {2017},
  month = dec,
  volume = {17},
  pages = {8027--8036},
  issn = {1530-437X, 1558-1748, 2379-9153},
  doi = {10.1109/JSEN.2017.2687828},
  abstract = {We describe an approach, based on electrical capacitance tomography (ECT) sensors, to decompose and continuously monitor multiphase flow components (fractional areas or volumes) in mixtures containing conducting phases. The proposed approach exploits the Maxwell\textendash Wagner\textendash Sillars effect at distinct frequencies to reconstruct each phase of a multiphase flow and is also utilized to estimate the fractional volume of the various phases of the mixture. The approach is illustrated for a three-phase mixture composed of air, water, and oil. This approach utilizes the very same ECT measurement apparatus used for flow imaging and, as such, inherits its high speed of acquisition and suitability for real-time operation.},
  journal = {IEEE Sensors Journal},
  keywords = {capacitance measurement,conducting phase,electric impedance imaging,Electrical capacitance tomography,electrical capacitance tomography sensors,flow visualisation,fractional volumes,image reconstruction,Image reconstruction,maxwel–Wagner–Sillars effect,Maxwell-Wagner-Sillars effect,multiphase flow,multiphase flow decomposition,phase reconstruction,Real-time systems},
  language = {en},
