@incollection{Thiele2007,
  title = {Development of a High-Speed Capacitive Surface Sensor for Fluid Distribution Imaging},
  booktitle = {2007 {{Ieee Sensors}}, {{Vols}} 1-3},
  author = {Thiele, Sebastian and Da Silva, Marco Jose and Hampel, Uwe},
  year = {2007},
  pages = {236--239},
  publisher = {{Ieee}},
  address = {{New York}},
  abstract = {Experimental investigation of multiphase flows plays an important role in many research and industrial application areas, such as chemical and petroleum industry and nuclear engineering. We introduce a novel surface sensor based on electrical capacitance (permittivity) measurements to visualize flows along the surface of objects. The prototype sensor is formed by a 32 x 32 pixel interdigital sensing structure and can be mounted onto the wall of pipes or vessels and thus has a minimal influence on the flow. An associated electronics measures the capacitance of the fluid on each sensing structure in a multiplexed manner. This way, images of the fluid distribution can be achieved directly without the need for any reconstruction procedure. Images at up to 10,000 frames per second can be obtained. The system was evaluated showing good reproducibility and accuracy. Initial results of a two-component mixing experiment are presented.},
  annotation = {WOS:000254563600062},
  isbn = {978-1-4244-1261-7},
  language = {English}
}

@article{Thiele2009,
