@article{Wang2019b,
  title = {Measurement of Flow Rate in Solid-Liquid Two-Phase Flow in Pipes at Low Volume Concentration with Venturimeter},
  author = {Wang, Liyan and Li, Saiwei and Yuan, Ye and Sun, Zhiqiang and Zhou, Tian},
  year = {2019},
  month = may,
