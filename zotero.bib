@article{Lindken2002,
  title = {A Novel {{PIV}} Technique for Measurements in Multiphase Flows and Its Application to Two-Phase Bubbly Flows},
  author = {Lindken, R. and Merzkirch, W.},
  year = {2002},
  month = dec,
  volume = {33},
