@article{Xu1997,
  title = {Gas/Liquid Two-Phase Flow Regime Identification by Ultrasonic Tomography},
  author = {Xu, L. J. and Xu, L. A.},
  year = {1997},
  month = apr,
  volume = {8},
  pages = {145--155},
  issn = {0955-5986},
  doi = {10.1016/S0955-5986(98)00002-8},
  abstract = {A gas/liquid two-phase flow is considered as a strongly inhomogeneous medium with respect to high contrast in acoustic impedance distribution. Based upon a binary logic operation and a method of ``time-of-propagation along straight path'', an ultrasonic facility for tomographic imaging of gas/liquid two-phase flow was developed. In this paper the principle and construction of this facility are briefly introduced. Emphasis is placed on the evaluation of its performance in flow regime identification and cross-sectional void fraction measurement. Several flow pattern models were used and the corresponding monitoring results given. Finally, limitations and possible future improvements of the system are discussed.},
