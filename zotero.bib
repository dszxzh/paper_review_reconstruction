@article{He2012,
  title = {{矿浆管道输送流速仿人智能多模态控制研究}},
  author = {何, 成 and 王, 耀南 and 邹, 伟生 and 成, 晓明},
  year = {2012},
  volume = {27},
  pages = {295--298+303},
  abstract = {针对矿浆管道输送流速控制的大时滞、建模困难等问题,提出分段变增量的仿人智能多模态控制新算法.将完整矿浆批量输送实际过程划分成多段特征模态,采用不同的控制增量算法以推理确定控制模态.基于新研制的多泵站管道输送平台,给出了系统结构框图与算法模块设计,采用两种矿浆管道批量输送流速运行方式,实现了满意的实时控制,验证了控制系统结构与算法的可行性和有效性.},
