@article{Bobade2019,
  title = {Bubble Rise Velocity and Bubble Size in Thickened Waste Activated Sludge: {{Utilising}} Electrical Resistance Tomography ({{ERT}})},
  shorttitle = {Bubble Rise Velocity and Bubble Size in Thickened Waste Activated Sludge},
  author = {Bobade, Veena and Evans, Geoffery and Eshtiaghi, Nicky},
  year = {2019},
  month = aug,
  volume = {148},
