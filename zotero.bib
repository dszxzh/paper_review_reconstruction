@article{Fabian1997,
  ids = {Fabian1997a},
  title = {A Robust Capacitive Angular Speed Sensor},
  author = {Fabian, T. and Brasseur, G.},
  year = {1997},
  volume = {2},
  pages = {280--284},
  abstract = {This paper presents a contactless capacitive angular speed sensor for automotive applications. The sensor is based on a passive rotating electrode placed between two mechanically static and electrically active electrodes. The different characteristics of the charge transfer at various sensor positions is utilized as an input for the calculation of the rotational speed. The main advantages of this low cost system are its capability to operate at high temperatures and humidity as well as its insensitivity to vibrations, dirt, dew and moisture deposited on the three sensor electrodes. The mathematical model of the sensor further enables the optimization of the sensor characteristics for specific applications. Experimental results from a prototype designed for the speed-measurement of a steering-wheel show a relative speed error of 卤4\% at a resolution better than 1掳/s},
  journal = {IEEE Transactions on Instrumentation \& Measurement},
  keywords = {angular velocity measurement,automotive applications,automotive electronics,charge transfer,contactless capacitive angular speed sensor,electric sensing devices,electrically active electrodes,mechanically static electrodes,optimisation,passive rotating electrode},
