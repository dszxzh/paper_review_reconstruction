@article{Wang2014b,
  title = {Electrical Capacitance Volume Tomography for Imaging of Pulsating Flows in a Trickle Bed},
  author = {Wang, Aining and Marashdeh, Qussai and Motil, Brian J. and Fan, Liang-Shih},
  year = {2014},
  month = nov,
  volume = {119},
