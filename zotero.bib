@article{Ahmad2019,
  title = {Extending the {{EMMS}}/Bubbling Model to Fluidization of Binary Particle Mixture: {{Formulation}} and Steady-State Validation},
  shorttitle = {Extending the {{EMMS}}/Bubbling Model to Fluidization of Binary Particle Mixture},
