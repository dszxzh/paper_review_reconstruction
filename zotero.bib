  author = {Li, Yi and Yang, Wuqiang and Tsamakis, Dimitrios and Wu, Zhipeng and Lenn, Chris and Xie, Chenggang and Huang, Songming and Cutler, Annette},
  year = {2010},
  month = jul,
  pages = {96--99},
  publisher = {{IEEE}},
  address = {{Thessaloniki, Greece}},
  doi = {10.1109/IST.2010.5548520},
  abstract = {This paper presents a model-based image reconstruction algorithm for the measurement of multiphase distributions, in particular the thickness of liquid layer of annular distribution and the water-to-liquid ratio (WLR) of oilcontinuous flows. A software package based on a finite element method (FEM) has been developed. The parameters of the simulated distribution are updated to minimize the discrepancies between the calculated and measured capacitances. To verify the algorithm, an impedance analyzer (HP4192) was used to measure capacitances from an 8-electrode ECT sensor with different thickness of the solids or liquid layer and different WLR. This algorithm has been applied for the measurement of gas-oil-water oil-continuous flows with WLR  30\%.},
  isbn = {978-1-4244-6492-0},
  language = {en}
}

@phdthesis{Li2012,
  title = {浆体管道输送系统的过程监控与故障诊断},
  author = {李, 祯},
  year = {2012},
  address = {{沈阳}},
  abstract = {浆体管道输送技术自上世纪中期由西方引入我国,由于其具有的节能、经济、环保等优点,随后在我国的矿山领域获得广泛的应用,并取得巨大的经济效益。采用过程监控的理论方法,搭建浆体管道输送过程监控系统符合现代化工业的发展趋势。过程监控中尤为重要的是对故障的诊断,能够及时有效的对故障进行识别、分析和处理,可以最大限度地保障生产过程的安全性,可以极大地提高生产效率。 本文首先分析浆体管道输送系统的运行机理,随后,针对具有不同功能的组成部分,提出采用不同的故障诊断方法对浆体管道输送系统进行故障诊断的思路。针对矿浆仓、清水池、隔离罐、水阀门等运行规律较稳定,并且容易采用现场仪器仪表进行检测的部分,提出采用解析的方法进行故障诊断;针对清水泵及外管道等变化参数多且难以建立精确数学模型的部分,提出采用BP人工神经网络的方法进行故障诊断,并利用现场获得的数据进行仿真,取得了良好的故障诊断效果。 最后,本文对浆体管道输送过程监控系统进行整体硬/软件设计,对上位机、下位机及现场检测仪器仪表进行硬/软件配置。采用现场总线的连接方式,初步完成硬件组态及软件组态,并完成部分监控画面,构建了功能较为全面的浆体管道输送过程监控系统。},
  keywords = {人工神经网络,故障诊断,浆体管道输送,过程监控},
  school = {沈阳大学}
}

