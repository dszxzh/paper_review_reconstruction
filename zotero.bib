@article{Hao2020,
  ids = {Hao2020a},
  title = {2-{{D Median Filter}}-{{Based Impulsive Noise Reduction}} in {{Multifrequency Phase}}-Sensitive {{Demodulation}} of {{Electrical Impedance Tomography}}},
  author = {Hao, Zhenhua and Cui, Ziqiang and Yue, Shihong and Wang, Huaxiang},
  year = {2020},
